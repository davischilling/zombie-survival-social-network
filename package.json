{
  "name": "zombie-survival-social-network",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules --clear -r dotenv/config src/server.ts",
    "test": "jest --passWithNoTests --no-cache --runInBand --setupFiles dotenv/config",
    "test:watch": "npm run test -- --watch",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:coverage": "npm run test -- --coverage",
    "test:clear-cache": "jest --clearCache",
    "lint": "eslint src --ext .tsx,.ts",
    "lint:fix": "eslint src tests --ext .tsx,.ts --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jest-mock/express": "^2.0.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "module-alias": "^2.2.2"
  }
}
